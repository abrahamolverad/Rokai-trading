# MongoDB connection string - replace with your actual MongoDB connection string
MONGODB_URI=mongodb://localhost:27017/ai_trading_platform

# JWT secret key - change this to a secure random string in production
JWT_SECRET=your_jwt_secret_key_change_this_in_production

# Server port
PORT=3000

# Node environment
NODE_ENV=development

# API rate limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# Bcrypt salt rounds
BCRYPT_SALT_ROUNDS=10

# Token expiration (in seconds)
TOKEN_EXPIRATION=86400

# Cors origins
CORS_ORIGIN=http://localhost:8080,https://aitrading.com
